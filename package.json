{
  "private": true,
  "name": "ka-mensa-api",
  "version": "0.0.0",
  "description": "Karlsruhe (KIT) Mensa API server",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && npm run production",
    "build": "rimraf dist && tsc",
    "production": "node dist/server.js",
    "lint": "eslint --ignore-path .gitignore . && tsc --noEmit",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && tsc --noEmit",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-api.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "api-server"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-api/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-api",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "fs-adapters": "6.0.2",
    "group-items": "2.0.0",
    "ka-mensa-fetch": "2.1.5",
    "moment": "2.29.1",
    "ms": "2.1.3",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.4",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.0",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.21",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "37.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "mocha": "9.2.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
